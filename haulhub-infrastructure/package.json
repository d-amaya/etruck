{
  "name": "@haulhub/infrastructure",
  "version": "1.0.0",
  "description": "HaulHub Infrastructure (AWS CDK)",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all --profile haul-hub",
    "deploy:dev": "cdk deploy --all -c environment=dev --profile haul-hub",
    "deploy:staging": "cdk deploy --all -c environment=staging --profile haul-hub",
    "deploy:prod": "cdk deploy --all -c environment=prod --profile haul-hub",
    "deploy:script": "./scripts/deploy.sh",
    "update:lambda": "./scripts/update-lambda.sh",
    "destroy": "cdk destroy --all --profile haul-hub",
    "bootstrap": "cdk bootstrap --profile haul-hub",
    "diff": "cdk diff --profile haul-hub",
    "list": "cdk list --profile haul-hub"
  },
  "dependencies": {
    "@haulhub/shared": "file:../haulhub-shared",
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "aws-cdk": "^2.120.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "haulhub",
    "infrastructure",
    "aws-cdk"
  ],
  "author": "",
  "license": "ISC"
}
