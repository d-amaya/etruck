{
  "name": "@haulhub/shared",
  "version": "1.0.0",
  "description": "Shared TypeScript interfaces and types for HaulHub",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "clean:src": "find src -name '*.js' -o -name '*.d.ts' -o -name '*.js.map' -o -name '*.d.ts.map' | xargs rm -f",
    "prebuild": "npm run clean:src",
    "test": "jest --no-cache"
  },
  "keywords": [
    "haulhub",
    "shared",
    "types"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "class-validator": "^0.14.2",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.2.2"
  }
}
